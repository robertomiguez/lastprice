{
  "name": "lastprice",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start -c",
    "android": "expo run:android -d",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "start:reset-cache": "expo start -- --reset-cache",
    "prebuild": "expo prebuild --clean",
    "cache:clean": "npm cache clean --force",
    "test": "jest --watchAll",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier:format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md,prettierrc}\"",
    "prettier:check": "prettier . --check",
    "prepare": "husky",
    "doctor": "npx expo-doctor",
    "expo:check": "npx expo install --check",
    "eas:secret:push": "eas secret:push --scope project --env-file .env --force",
    "eas:secret:list": "eas secret:list"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-ml-kit/text-recognition": "^1.5.2",
    "@react-navigation/bottom-tabs": "^7.4.2",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/native-stack": "^7.3.21",
    "axios": "^1.10.0",
    "expo": "~53.0.20",
    "expo-camera": "~16.1.11",
    "expo-image-picker": "~16.1.4",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.10",
    "husky": "^9.1.7",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "expo-linear-gradient": "~14.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~19.0.10",
    "typescript": "~5.8.3"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "private": true
}
